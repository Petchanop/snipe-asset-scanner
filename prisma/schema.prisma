generator client {
  provider               = "prisma-client-js"
  output                 = "./generated/prisma"
  generatedFileExtension = "ts"
  importFileExtension    = "ts"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model accessories {
  id              Int       @id @default(autoincrement()) @db.UnsignedInt
  name            String?
  category_id     Int?
  created_by      Int?
  qty             Int       @default(0)
  requestable     Boolean   @default(false)
  created_at      DateTime? @db.Timestamp(0)
  updated_at      DateTime? @db.Timestamp(0)
  deleted_at      DateTime? @db.Timestamp(0)
  location_id     Int?
  purchase_date   DateTime? @db.Date
  purchase_cost   Decimal?  @db.Decimal(20, 2)
  order_number    String?
  company_id      Int?      @db.UnsignedInt
  min_amt         Int?
  manufacturer_id Int?
  model_number    String?
  image           String?
  supplier_id     Int?
  notes           String?   @db.Text

  @@index([company_id], map: "accessories_company_id_index")
}

model accessories_checkout {
  id            Int       @id @default(autoincrement()) @db.UnsignedInt
  created_by    Int?
